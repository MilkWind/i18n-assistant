  "i18n_path": "D:/development/app-projects/questions-party/questions-party-backend/src/locales",
  "output_path": "./i18n-analysis",
  "ignore_patterns": [
    "node_modules/**",
    ".git/**",
    "dist/**",
    "build/**",
    "__pycache__/**",
    ".vscode/**",
    ".idea/**",
    ".venv/**"
